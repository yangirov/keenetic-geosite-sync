{
	"name": "keenetic-geosite-sync",
	"description": "Sync Keenetic DNS object-groups from v2fly geosite lists",
	"author": "Emil Yangirov",
	"license": "MIT",
	"version": "1.0.3",
	"main": "dist/index.js",
	"bin": "dist/index.js",
	"scripts": {
		"build": "tsup && cp config.json dist/config.json && cp -r scripts/ dist/scripts",
		"dev": "tsup --watch",
		"lint": "npx tsc --noEmit && biome check .",
		"lint:fix": "npx tsc --noEmit && biome check --write --unsafe .",
		"start": "node dist/index.js",
		"test": "npm run build && vitest run",
		"test:watch": "vitest watch"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.3",
		"@types/node": "^22.0.0",
		"tsup": "^7.2.0",
		"typescript": "^5.4.0",
		"vitest": "^1.6.0"
	}
}
